  "bugs": {
    "url": "https://github.com/t4top/mina-wordle-game/issues"
  },
  "homepage": "https://github.com/t4top/mina-wordle-game#readme",
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.11.12",
    "pm2": "^5.2.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "axios": "^1.2.1",
